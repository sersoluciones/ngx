{
  "name": "@sersol/ngx",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr -o --ssl --ssl-key localhost.key  --ssl-cert localhost.crt",
    "start-plain": "ng serve --hmr -o --host 0.0.0.0",
    "build": "ng build ngx --configuration production",
    "publish-lib": "npm run build && npm publish ./dist --access public && npm run publish-doc && npm run publish-showcase",
    "publish-doc": "npx compodoc -p tsconfig.json -d ../ngx-doc --theme vagrant --language es-ES src",
    "commit": "git --C=../../ngx-doc commit -m 'Auto publish' && git --C=../../ngx-doc push",
    "publish-showcase": "ng deploy showcase --base-href=/ngx/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^13.2.4",
    "@angular/cdk": "^13.2.4",
    "@angular/common": "^13.2.4",
    "@angular/compiler": "^13.2.4",
    "@angular/core": "^13.2.4",
    "@angular/forms": "^13.2.4",
    "@angular/material": "^13.2.4",
    "@angular/platform-browser": "^13.2.4",
    "@angular/platform-browser-dynamic": "^13.2.4",
    "@angular/router": "^13.2.4",
    "@splidejs/splide": "^3.6.12",
    "angular-cli-ghpages": "1.0.0",
    "bowser": "^2.11.0",
    "compressorjs": "^1.1.1",
    "highlight.js": "^11.4.0",
    "ngx-highlightjs": "^6.1.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1302.5",
    "@angular-devkit/build-angular": "^13.2.5",
    "@angular/cli": "^13.2.5",
    "@angular/compiler-cli": "^13.2.4",
    "@angular/language-service": "^13.2.4",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/gapi": "0.0.41",
    "@types/gapi.auth2": "0.0.56",
    "@types/highlight.js": "^10.1.0",
    "@types/jasmine": "~3.3.16",
    "@types/node": "^12.20.46",
    "jasmine-core": "~3.5.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "typescript": "~4.5.2"
  }
}
